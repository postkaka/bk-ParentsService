<!--components/wx-class-item/wx-class-item.wxml-->
<view class="class-list" style="height:{{classlistheight}}rpx">
   <view class="class-item-left">
       <view class="class-item-left-image" style="display:{{imagedisplay}}">
           <image src="{{classitemimage}}"mode="widthFix" style="width: 88rpx" class="class-item-left-image-icon"></image>
       </view>
       <view class="class-item-left-images">
           <view>{{classitemleftview}}</view>
           <view>
               <view >
                   <image src="{{classitemlefticon}}" style="width: 28rpx;height:28rpx"></image>
               </view>
               <view>{{classitemlefttime}}</view>
           </view>
       </view>
   </view>
   <view class="class-item-right">
       <view></view>
       <view></view>
   </view>
</view>
