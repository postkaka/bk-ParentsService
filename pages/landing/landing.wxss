/* pages/landing/landing.wxss */
 page {
     overflow: hidden
 }
 .page {
    width: 100vw;
    height: 100%;
 }
.background {
    display: flex;
    align-content: center;
    justify-content: center;
}
.logo {
    width: 100%;
    text-align: center;
    position: absolute;
    top: 300rpx;
}
.list {
    position: absolute;
    top: 650rpx;
    border: 1rpx solid #ffffff;
    background-color:#ffffff;
    width: 686rpx;
    height: 566rpx;
    border-radius: 30rpx;
    margin-left: 32rpx;
    margin-right: 32rpx;
}
.lists {
    margin-left: 32rpx;
    margin-right: 32rpx;
}
.text {
    color: #F25A43;
    font-size: 34rpx;
    line-height: 48rpx;
}
.textitem {
    color: #333333;
    font-family: PingFang SC;
    font-size: 34rpx;
    line-height: 48rpx;
}

.texts {
    margin-top: 32rpx;
    margin-bottom: 32rpx;
}
.input {
    border: 1rpx solid #DDDDDD;
    height: 72rpx;
    border-radius: 40rpx;
    display: flex;
}
.input-text {
    height: 72rpx;
    margin-left: 30rpx;
    color: #dddddd;
    font-size: 28rpx;
}
.input-note {
    height: 72rpx;
    line-height: 72rpx;
    color: #F25A43;
    font-size: 28rpx;
    margin-left: 120rpx;
}
.button {
    height: 80rpx;
    line-height: 80rpx;
    background-color: #184571;
    margin-top: 63rpx;
    margin-left: 32rpx;
    margin-right: 32rpx;
    text-align: center;
    color: #FFFFFF;
    font-size: 34rpx;
    font-family: PingFang SC;
    border-radius: 30rpx;
}